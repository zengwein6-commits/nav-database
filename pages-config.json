{
  "build": {
    "command": "",
    "outputDirectory": ""
  },
  "headers": [
    {
      "source": "/**",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
